{
	"name": "quill-lang",
	"displayName": "Quill Language",
	"description": "Syntax highlighting for the Quill language",
	"version": "0.1.0",
	"publisher": "brianush1",
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "ql",
				"aliases": [
					"Quill Language",
					"ql"
				],
				"extensions": [
					".ql"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ql",
				"scopeName": "source.quill",
				"path": "./syntaxes/quill.tmLanguage"
			}
		]
	}
}